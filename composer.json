{
    "name": "xaboy/laravel-form-builder",
    "description": "laravel专用表单生成器，快速生成现代化的form表单。包含复选框、单选框、输入框、下拉选择框等元素以及,省市区三级联动,时间选择,日期选择,颜色选择,文件/图片上传等功能。",
    "license": "MIT",
    "keywords": ["form","form-builder","form-generator","iview","laravel"],
    "homepage": "https://github.com/xaboy/laravel-form-builder",
    "authors": [
        {
            "name": "xaboy",
            "email": "xaboy2005@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelFormBuilder\\": "./src/"
        }
    },
    "support": {
        "issues": "https://github.com/xaboy/laravel-form-builder/issues",
        "source": "https://github.com/xaboy/laravel-form-builder"
    },
    "require": {
        "php": ">=5.4.0",
        "xaboy/form-builder": "^1.2.7",
        "laravel/framework": "^5.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFormBuilder\\FormBuilderProvider"
            ]
        }
    }
}
